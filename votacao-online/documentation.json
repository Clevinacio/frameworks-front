            "templateData": "<div class=\"container\">\r\n  <div class=\"question\">\r\n    <h2>{{pergunta}}</h2>\r\n  </div>\r\n  <div *ngIf=\"mode === 'open'; else closed\">\r\n    <cabine [opcoes]=\"opcoes\" (click)=\"onClick($event); result()\">\r\n    </cabine>\r\n  </div>\r\n\r\n  <ng-template #closed>\r\n    <div>\r\n      <resultado [opcoes]=\"opcoes\" [totalCount]=\"totalVotes\"></resultado>\r\n    </div>\r\n  </ng-template>\r\n\r\n\r\n</div>"
